      <VBox alignment="CENTER" spacing="25.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label styleClass="header" text="%marketing.pressRel.newTitle" />
            <ListView fx:id="newPressReleaseOptionsList" prefWidth="700.0" />
        	<Button fx:id="publishBtn" styleClass="button_module" text="%marketing.pressRel.publishBtn" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
    </children>
   <stylesheets>
      <URL value="@../../css/general.css" />
