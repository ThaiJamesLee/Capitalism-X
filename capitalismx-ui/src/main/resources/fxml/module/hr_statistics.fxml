<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uni.mannheim.capitalismx.ui.controller.module.hr.HrStatisticsController">
   <children>
      <VBox spacing="25.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <GridPane hgap="10.0" vgap="15.0">
              <columnConstraints>
                  <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
                  <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
                  <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints vgrow="SOMETIMES" />
                <RowConstraints vgrow="SOMETIMES" />
                <RowConstraints vgrow="SOMETIMES" />
                  <RowConstraints vgrow="SOMETIMES" />
                  <RowConstraints vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label styleClass="label_large" text="Employees" GridPane.rowIndex="1" />
                  <Label styleClass="label_large" text="Satisfaction" GridPane.rowIndex="2" />
                  <Label styleClass="label_large" text="Productivity" GridPane.rowIndex="3" />
                  <Label styleClass="label_large" text="Salespeople" GridPane.columnIndex="1" />
                  <Label styleClass="label_large" text="Engineers" GridPane.columnIndex="2" />
                  <Label fx:id="numberEmployeesSales" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="numberEmployeesOverall" text="Label" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                  <Label fx:id="employeeSatisfactionSales" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label fx:id="employeeSatisfactionOverall" text="Label" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                  <Label styleClass="label_large" text="Salaries" GridPane.rowIndex="4" />
                  <Label styleClass="label_large" text="Overall" GridPane.columnIndex="3" />
                  <Label fx:id="employeeSalariesProduction" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                  <Label fx:id="employeeSalariesSales" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label fx:id="employeeProductivityProduction" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                  <Label fx:id="employeeProductivitySales" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label fx:id="employeeProductivityOverall" text="Label" GridPane.columnIndex="3" GridPane.rowIndex="3" />
                  <Label fx:id="employeeSatisfactionProduction" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                  <Label fx:id="numberEmployeesProduction" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <Label fx:id="employeeSalariesOverall" text="Label" GridPane.columnIndex="3" GridPane.rowIndex="4" />
               </children>
            </GridPane>
            <Region styleClass="separator_horizontal" />
            <GridPane vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" />
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints vgrow="SOMETIMES" />
                <RowConstraints vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label styleClass="label_large" text="Hiring Cost" />
                  <Label styleClass="label_large" text="Firing Cost" GridPane.rowIndex="1" />
                  <Label fx:id="hiringCost" styleClass="label_large" text="Label" GridPane.columnIndex="1" />
                  <Label fx:id="firingCost" styleClass="label_large" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
               <padding>
                  <Insets left="15.0" right="15.0" />
               </padding>
            </GridPane>
         </children>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@../../css/general.css" />
      <URL value="@../../css/modul.css" />
   </stylesheets>
</AnchorPane>
