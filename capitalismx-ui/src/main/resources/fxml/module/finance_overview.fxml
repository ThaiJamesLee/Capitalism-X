<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uni.mannheim.capitalismx.ui.controller.module.finance.FinanceOverviewController">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Company">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Label>
            <GridPane>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Cash" />
                  <Label text="Assets" GridPane.rowIndex="1" />
                  <Label text="Liabilities" GridPane.rowIndex="2" />
                  <Label text="Net Worth" GridPane.rowIndex="3" />
                  <Label fx:id="cashLabel" layoutX="10.0" layoutY="107.0" GridPane.columnIndex="1" />
                  <Label fx:id="assetsLabel" layoutX="110.0" layoutY="17.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="liabilitiesLabel" layoutX="10.0" layoutY="17.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label fx:id="netWorthLabel" layoutX="10.0" layoutY="17.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
               </children>
            </GridPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
