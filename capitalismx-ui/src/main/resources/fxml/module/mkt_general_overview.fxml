<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uni.mannheim.capitalismx.ui.controller.module.marketing.MarketingOverviewController">
	<children>
      <GridPane hgap="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" />
            <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
      				<GridPane alignment="CENTER_LEFT" hgap="15.0" vgap="20.0">
      					<columnConstraints>
      						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      					</columnConstraints>
      					<rowConstraints>
      						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      					</rowConstraints>
      					<children>
      						<Label fx:id="compImTitleLbl" styleClass="label_large" text="%marketing.overview.companyImage">
						</Label>
      						<Label fx:id="compImValueLbl" styleClass="label_large" text="Label" GridPane.columnIndex="1">
						</Label>
      						<Label fx:id="emplBrTitleLbl" styleClass="label_large" text="%marketing.overview.employerBranding" GridPane.rowIndex="1">
						</Label>
      						<Label fx:id="emplBrValueLbl" styleClass="label_large" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1">
						</Label>
      					</children>
      				</GridPane>
      				<GridPane alignment="CENTER_RIGHT" hgap="10.0" vgap="20.0" GridPane.columnIndex="1">
      					<columnConstraints>
      						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      					</columnConstraints>
      					<rowConstraints>
      						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      					</rowConstraints>
      					<children>
      						<Label fx:id="consTitleLbl" styleClass="label_large" text="%marketing.overview.consultancyTitle" />
      						<Label fx:id="consTextLbl" styleClass="label_large" text="Explanation Consultancies" GridPane.rowIndex="1" />
      						<Label fx:id="consCurrentLbl" styleClass="label_large" text="None" GridPane.columnIndex="1" />
      						<Button fx:id="consChangeBtn" mnemonicParsing="false" styleClass="btn_standard" text="%marketing.overview.consChangeBtn" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      					</children>
      				</GridPane>
         </children>
         <padding>
            <Insets bottom="24.0" left="24.0" right="24.0" top="24.0" />
         </padding>
      </GridPane>
	</children>
	<stylesheets>
		<URL value="@../../css/general.css" />
		<URL value="@../../css/modul.css" />
	</stylesheets>
</AnchorPane>
