<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="de.uni.mannheim.capitalismx.ui.controller.module.marketing.MarketingOverviewController">
	<children>
		<HBox layoutX="200.0" layoutY="150.0" spacing="20.0"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<GridPane alignment="CENTER_LEFT" hgap="15.0" vgap="20.0">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="100.0" />
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="100.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
					</rowConstraints>
					<children>
						<Label fx:id="compImTitleLbl"
							text="%marketing.overview.companyImage">
							<font>
								<Font size="18.0" />
							</font>
						</Label>
						<Label fx:id="compImValueLbl" text="Label"
							GridPane.columnIndex="1">
							<font>
								<Font size="18.0" />
							</font>
						</Label>
						<Label fx:id="emplBrTitleLbl"
							text="%marketing.overview.employerBranding" GridPane.rowIndex="1">
							<font>
								<Font size="18.0" />
							</font>
						</Label>
						<Label fx:id="emplBrValueLbl" text="Label"
							GridPane.columnIndex="1" GridPane.rowIndex="1">
							<font>
								<Font size="18.0" />
							</font>
						</Label>
					</children>
				</GridPane>
				<GridPane alignment="CENTER_RIGHT" hgap="10.0" vgap="20.0">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="100.0" />
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="100.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
					</rowConstraints>
					<children>
						<Label fx:id="consTitleLbl"
							text="%marketing.overview.consultancyTitle" />
						<Label fx:id="consTextLbl" text="Explanation Consultancies"
							GridPane.rowIndex="1" />
						<Label fx:id="consCurrentLbl" text="Label"
							GridPane.columnIndex="1" />
						<Button fx:id="consChangeBtn" mnemonicParsing="false"
							styleClass="button_module"
							text="%marketing.overview.consChangeBtn" GridPane.columnIndex="1"
							GridPane.rowIndex="1" />
					</children>
				</GridPane>
			</children>
		</HBox>
	</children>
	<stylesheets>
		<URL value="@../../css/general.css" />
		<URL value="@../../css/modul.css" />
	</stylesheets>
</AnchorPane>
