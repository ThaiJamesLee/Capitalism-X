<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uni.mannheim.capitalismx.ui.controller.module.marketing.MarketingOverviewController">
   <children>
      <HBox layoutX="200.0" layoutY="150.0" prefHeight="100.0" prefWidth="200.0">
         <children>
            <GridPane>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label fx:id="compImTitleLbl" text="%marketing.overview.companyImage" />
                  <Label fx:id="compImValueLbl" text="Label" GridPane.columnIndex="1" />
                  <Label fx:id="emplBrTitleLbl" text="%marketing.overview.employerBranding" GridPane.rowIndex="1" />
                  <Label fx:id="emplBrValueLbl" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <GridPane>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label fx:id="consTitleLbl" text="%marketing.overview.consultancyTitle" />
                  <Label fx:id="consTextLbl" text="Explanation Consultancies" GridPane.rowIndex="1" />
                  <Label fx:id="consCurrentLbl" text="Label" GridPane.columnIndex="1" />
                  <Button fx:id="consChangeBtn" mnemonicParsing="false" text="%marketing.overview.consChangeBtn" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
