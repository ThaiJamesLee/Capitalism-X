<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="contentStack" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uni.mannheim.capitalismx.ui.controller.gamepage.GamePageController">
   <children>
      <AnchorPane fx:id="menuLayer" />
      <AnchorPane fx:id="messageLayer" StackPane.alignment="CENTER" />
      <AnchorPane fx:id="mapLayer" />
      <AnchorPane fx:id="moduleLayer" />
      <AnchorPane fx:id="hudLayer">
         <children>
            <fx:include source="hud.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
