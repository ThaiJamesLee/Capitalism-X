<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:id="gridPane" hgap="3.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="30.0" />
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="35.0" />
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="35.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Label fx:id="nameLabel" styleClass="list-label" text="Label">
            <font>
                <Font name="Arial Bold" size="12.0" />
            </font>
        </Label>
          <Label fx:id="priceLabel" styleClass="list-label" text="Label" GridPane.columnIndex="1">
              <font>
                  <Font name="Arial Bold" size="12.0" />
              </font>
          </Label>
      <Label fx:id="ecoIndexLabel" styleClass="list-label" text="Label" GridPane.columnIndex="2">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="qualityIndexLabel" layoutX="103.0" layoutY="17.0" styleClass="list-label" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="deliveryCostsLabel" styleClass="list-label" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="1">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font>
      </Label>
      <FontAwesomeIcon iconName="TRUCK" size="1.3em" GridPane.rowIndex="1" />
    </children>
</GridPane>
