<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="250.0" layoutY="65.0" spacing="8.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane fx:id="listAnchor" />
            <Region styleClass="separator_horizontal" />
            <Label fx:id="teamName" styleClass="label_very_large" text="Label" />
            <Label fx:id="teamBenefits" text="Label" />
            <GridPane hgap="16.0" vgap="8.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" />
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" percentWidth="30.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints vgrow="SOMETIMES" />
                  <RowConstraints vgrow="SOMETIMES" />
                <RowConstraints vgrow="SOMETIMES" />
                <RowConstraints vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label styleClass="label_large" text="Number of Employees" />
                  <Label styleClass="label_large" text="Satisfaction" GridPane.rowIndex="1" />
                  <Label styleClass="label_large" text="Productivity" GridPane.rowIndex="2" />
                  <Label styleClass="label_large" text="Salary Expenses" GridPane.rowIndex="3" />
                  <Label fx:id="number" contentDisplay="RIGHT" styleClass="label_large" text="Label" GridPane.columnIndex="1" />
                  <Label fx:id="satisfaction" contentDisplay="RIGHT" styleClass="label_large" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="productivity" contentDisplay="RIGHT" styleClass="label_large" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label fx:id="salary" contentDisplay="RIGHT" styleClass="label_large" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
               </children>
               <VBox.margin>
                  <Insets top="8.0" />
               </VBox.margin>
            </GridPane>
         </children>
         <padding>
            <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
